{
  "description": "A Docker volume plugin for MinFS",
  "documentation": "https://docs.docker.com/engine/extend/plugins/",
  "entrypoint": [
    "/usr/sbin/minfs-docker-plugin"
  ],
  "network": {
    "type": "host"
  },
  "interface": {
    "types": [
      "docker.volumedriver/1.0"
    ],
    "socket": "minfs.sock"
  },
  "linux": {
    "capabilities": [
      "CAP_SYS_ADMIN"
    ],
    "devices": [
      {
        "path": "/dev/fuse"
      }
    ]
  },
  "propagatedmount": "/tmp",
  "env": [
    {
      "name": "DEBUG",
      "settable": [
        "value"
      ],
      "value": "0"
    }
  ]
}
